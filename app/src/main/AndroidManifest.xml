<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mylevering">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeLanding"
            android:screenOrientation="locked">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LogIn"
            android:label="@string/title_activity_login"
            android:screenOrientation="locked">
        </activity>
        <activity
            android:name=".SignUp"
            android:label="@string/title_activity_signup"
            android:screenOrientation="locked">
        </activity>
        <activity
            android:name=".ButterflyMenuItem"
            android:label="@string/menu_item"
            android:parentActivityName=".ButterflyOrder"
            android:screenOrientation="locked">
        </activity>
        <activity
            android:name=".ButterflyOrder"
            android:label="@string/butterfly"
            android:launchMode="singleTop"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="locked">
        </activity>
        <activity
            android:name=".ConfirmOrder"
            android:label="@string/confirm_order_label"
            android:screenOrientation="locked"/>
        <activity
            android:name=".FreshOrder"
            android:label="@string/freshLabel"
            android:launchMode="singleTop"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="locked"/>
        <activity
            android:name=".MainActivity"
            android:label="@string/kitchens"
            android:launchMode="singleTop"
            android:theme="@style/AppTheme.Settings"
            android:screenOrientation="locked">
        </activity>
    </application>

</manifest>